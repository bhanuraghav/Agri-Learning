{
  "name": "paytm-hackathon",
  "version": "1.0.0",
  "description": "A web application that uses a CNN model to predict pests and crop diseases and provide the required measures to help farmers",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:dev": "nodemon app.js",
    "build:css": "node-sass --include-path sass public/css/scss/main.scss public/css/styles.css",
    "watch:css": "nodemon -e scss -x \"npm run build:css\"",
