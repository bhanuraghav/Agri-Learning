    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "form-data": "^2.3.3",
    "mongoose": "^5.4.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-sass": "^4.11.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
